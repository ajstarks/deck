deck
	slide "red" "white"
		ctext "First Slide" 50 50 10
 	eslide

	slide "red" "white"
		ctext "Slide 2" 50 50 10
	eslide

	slide "blue" "white"
		ctext "Slide 3" 50 50 10
	eslide

	slide "green" "white"
		ctext "Slide 4" 50 50 10
	eslide

	slide "orange"
		ctext "Slide 5" 50 50 10
 	eslide
	slide
	 	for x=5 95 5
		 	circle x 50 4 "maroon" x
		efor
		for y=5 95 5
			circle 50 y 4 "darkblue" y
		efor
		etext "Slide 6" 95 5 2
	eslide

	opts="-fulldeck=f -xlabel=5 -yaxis -yrange=0,100,20 -grid -val -bar -val=f -min=0 -max=100 -vcolor=maroon -datacond=20,50,orange"
	slide
		dchart opts rand.d
		etext "Slide 7" 95 5 2
	eslide

	slide
		ctext "Slide 8" 50 50 10
	eslide

	slide
		ctext "Slide 9" 50 50 10
 	eslide

	slide "purple" "white"
		ctext "Slide 10" 50 50 10
	eslide
edeck
